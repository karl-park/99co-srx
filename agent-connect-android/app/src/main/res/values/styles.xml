<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.DarkActionBar.Bridge">
        <item name="android:windowBackground">@color/white_invertible</item>
        <item name="colorAccent">@color/selected</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="appCardViewStyle">@style/AppCardView</item>
        <item name="otpViewStyle">@style/OtpWidget.OtpView</item>
        <item name="subFloatingActionButtonStyle">@style/SubFloatingActionButton</item>
        <item name="actionMenuTextColor">@color/selected</item>
        <item name="android:actionMenuTextColor">@color/selected</item>
        <item name="android:titleTextColor">@color/black_invertible</item>
        <item name="titleTextColor">@color/black_invertible</item>
        <item name="android:subtitleTextColor">@color/gray_invertible</item>
        <item name="subtitleTextColor">@color/gray_invertible</item>
        <item name="android:actionButtonStyle">@style/AppActionButton</item>
        <item name="actionButtonStyle">@style/AppActionButton</item>
        <item name="pillStyle">@style/Pill</item>
        <item name="smallPillStyle">@style/SmallPill</item>
        <item name="clickableTextStyle">@style/ClickableText</item>
        <item name="searchButtonStyle">@style/SearchButton</item>
        <item name="tabButtonCenterStyle">@style/TabButtonCenter</item>
        <item name="tabLeftButtonStyle">@style/TabButtonLeft</item>
        <item name="tabRightButtonStyle">@style/TabButtonRight</item>
    </style>

    <!--Action bar-->
    <style name="ClassicActionBar" parent="Widget.AppCompat.Toolbar">
        <item name="navigationIcon">@drawable/ic_arrow_left</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:drawableTint">@color/black_invertible</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">@color/white_invertible</item>
        <item name="titleTextAppearance">@style/Title</item>
    </style>

    <!--Action button-->
    <style name="AppActionButton" parent="Widget.AppCompat.ActionButton">
        <item name="textAllCaps">false</item>
    </style>

    <!-- Dialog -->
    <style name="FullScreenDialogAnimation">
        <item name="android:interpolator">@android:anim/accelerate_interpolator</item>
        <item name="android:windowEnterAnimation">@anim/activity_slide_up</item>
        <item name="android:windowExitAnimation">@anim/activity_slide_down</item>
    </style>

    <style name="NonFullScreenDialogAnimation">
        <item name="android:windowIsFloating">true</item>
        <item name="android:interpolator">@android:anim/accelerate_interpolator</item>
        <item name="android:windowEnterAnimation">@anim/activity_slide_up</item>
        <item name="android:windowExitAnimation">@anim/activity_slide_down</item>
    </style>

    <style name="CustomAlertDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/accordion_background</item>
    </style>

    <style name="FullScreenAlertDialog">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:windowBackground">@color/transparent</item>
    </style>

    <style name="FullScreenWhiteBackgroundAlertDialog" parent="FullScreenAlertDialog">
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="BigHeadingColor" parent="Heading2Color">
        <item name="android:textSize">@dimen/text_size_22</item>
    </style>

    <!-- Text Styles defined from srx design guidelines -->
    <!-- Used only for property page title -->
    <style name="Heading1" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textSize">@dimen/text_size_24</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
        <item name="android:textColor">@color/black_invertible</item>
    </style>

    <style name="Heading2" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_18</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
        <item name="android:textColor">@color/black_invertible</item>
    </style>

    <style name="Heading2Color" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_18</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
        <item name="android:textColor">@color/purple</item>
    </style>

    <!-- for title, label & currency -->
    <style name="Title" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
        <item name="android:textColor">@color/black_invertible</item>
    </style>

    <style name="TitleColor" parent="Title">
        <item name="android:textColor">@color/purple</item>
    </style>

    <style name="Body" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="BodyColor" parent="Body">
        <item name="android:textColor">@color/purple</item>
    </style>

    <style name="BodyGray" parent="Body">
        <item name="android:textColor">@color/gray_invertible</item>
    </style>

    <style name="BodyBold" parent="Body">
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
    </style>

    <style name="BodyColorBold" parent="BodyColor">
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
    </style>

    <style name="BodyRed" parent="Body">
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="BodyOrange" parent="Body">
        <item name="android:textColor">@color/orange_light</item>
    </style>

    <style name="SmallBody" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textColor">@color/black_invertible</item>
    </style>

    <style name="SmallBodyBold" parent="SmallBody">
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
    </style>

    <style name="SmallBodyColorBold" parent="SmallBody">
        <item name="android:textColor">@color/purple</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
    </style>

    <style name="SmallBodyCyan" parent="SmallBody">
        <item name="android:textColor">@color/cyan</item>
        <item name="android:drawableTint">@color/cyan</item>
    </style>

    <style name="SmallBodyPurple" parent="SmallBody">
        <item name="android:textColor">@color/purple</item>
        <item name="android:drawableTint">@color/purple</item>
    </style>

    <style name="SmallBodyCyanBold" parent="SmallBodyCyan">
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
    </style>

    <style name="SmallBodyGray" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textColor">@color/gray_invertible</item>
    </style>

    <style name="SmallBodyOrange" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textColor">@color/orange_light_invertible</item>
    </style>

    <style name="SmallBodyError" parent="SmallBody">
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="SubText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textColor">@color/gray_invertible</item>
    </style>

    <style name="ExtraSmallBody" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:textColor">@color/black_invertible</item>
    </style>

    <style name="ExtraSmallBodyOrange" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:textColor">@color/orange_light_invertible</item>
    </style>

    <style name="ExtraSmallBodyBold" parent="ExtraSmallBody">
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
    </style>

    <style name="ExtraSmallBodyGray" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:textColor">@color/gray_invertible</item>
    </style>

    <style name="ExtraSmallBodyPurple" parent="ExtraSmallBody">
        <item name="android:textColor">@color/purple</item>
    </style>

    <style name="ExtraSmallBodyError" parent="ExtraSmallBody">
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="Menu" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/text_size_10</item>
        <item name="android:textColor">@color/black_invertible</item>
    </style>

    <style name="MenuPurple" parent="Menu">
        <item name="android:textColor">@color/purple</item>
    </style>

    <style name="CertifiedListingWhiteTag" parent="Menu">
        <item name="android:drawablePadding">@dimen/spacing_xxs</item>
        <item name="android:drawableStart">@drawable/ic_small_certified_listing</item>
        <item name="android:textColor">@color/white_invertible</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/label_certified_listing</item>
    </style>

    <!-- Text Input or Edit Text Style By design guidelines -->
    <style name="EditText" parent="Widget.Design.TextInputLayout">
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="colorControlActivated">@color/gray_invertible</item>
        <item name="android:backgroundTint">@color/black_invertible</item>
    </style>

    <style name="RoundedEditText" parent="Widget.Design.TextInputLayout">
        <item name="android:minHeight">@dimen/size_40</item>
        <item name="android:background">@drawable/et_rounded_gray_background_with_border</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:paddingStart">@dimen/spacing_m</item>
        <item name="android:paddingEnd">@dimen/spacing_s</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:drawableTint">@color/black_invertible</item>
        <item name="android:textColorHint">@color/text_box_hint</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="RoundedEditText.Small" parent="RoundedEditText">
        <item name="android:background">@drawable/et_rounded_gray_background_with_border_small
        </item>
    </style>

    <style name="RoundedMessagingEditText" parent="Widget.Design.TextInputLayout">
        <item name="android:background">@color/transparent</item>
        <item name="android:maxHeight">@dimen/size_messaging_text_area</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:paddingTop">@dimen/spacing_xs</item>
        <item name="android:paddingBottom">@dimen/spacing_xs</item>
        <item name="android:paddingStart">@dimen/spacing_m</item>
        <item name="android:paddingEnd">@dimen/spacing_s</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColorHint">@color/text_box_hint</item>
    </style>

    <!-- When text box is empty, it looks like it is already occupied -->
    <!-- Leave it blank will submit default value -->
    <style name="PillEditText" parent="RoundedEditText">
        <item name="android:textColor">@color/transparent</item>
        <item name="android:textColorHint">@color/text_box_hint</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:background">@drawable/et_rounded_gray_background_with_border_overlay
        </item>
    </style>

    <style name="EditTextPillNumberDisplay" parent="RoundedEditText">
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:textColorHint">@color/text_box_hint</item>
        <item name="android:cursorVisible">true</item>
        <item name="android:background">@drawable/et_rounded_gray_background_with_border_overlay
        </item>
    </style>

    <style name="SearchEditText" parent="Widget.Design.TextInputLayout">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:minHeight">@dimen/size_40</item>
        <item name="android:background">@drawable/et_search</item>
        <item name="android:textColorHint">@color/text_box_hint</item>
        <item name="android:drawableTint">@color/text_box_hint</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:drawableStart">@drawable/ic_search</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:paddingStart">@dimen/spacing_s</item>
        <item name="android:paddingEnd">@dimen/spacing_m</item>
        <item name="android:drawablePadding">@dimen/spacing_m</item>
        <item name="android:imeOptions">actionSearch</item>
    </style>

    <style name="SearchEditText.Disabled" parent="SearchEditText">

    </style>

    <style name="SearchEditText.Pseudo" parent="SearchEditText">
        <item name="android:stateListAnimator">@null</item>
    </style>

    <!-- Clickable text link -->
    <style name="ClickableText" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:textColor">@color/tv_clickable_text_color</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:drawableTint">@color/light_gray_invertible</item>
        <item name="android:drawablePadding">@dimen/spacing_xs</item>
    </style>

    <!--Buttons Style-->
    <style name="PrimaryButton" parent="TextAppearance.AppCompat.Widget.Button">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:background">@drawable/btn_rounded_yellow_background</item>
        <item name="android:minHeight">@dimen/btn_height</item>
        <item name="android:textColor">@color/text_primary_button</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">@integer/font_weight_semi_bold
        </item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:layout_height">@dimen/btn_height</item>
        <item name="android:paddingStart">@dimen/spacing_l</item>
        <item name="android:paddingEnd">@dimen/spacing_l</item>
    </style>

    <style name="PrimaryButtonWithIcon" parent="PrimaryButton">
        <item name="android:paddingStart">@dimen/spacing_m</item>
        <item name="android:paddingEnd">@dimen/spacing_l</item>
    </style>

    <style name="SecondaryButton" parent="TextAppearance.AppCompat.Widget.Button">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:background">@drawable/btn_secondary</item>
        <item name="android:minHeight">@dimen/btn_height</item>
        <item name="android:textColor">@color/text_secondary_button</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/text_size_18</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">@dimen/spacing_l</item>
        <item name="android:paddingStart">@dimen/spacing_l</item>
        <item name="android:paddingEnd">@dimen/spacing_l</item>
    </style>

    <style name="SearchButton" parent="TextAppearance.AppCompat.Widget.Button">
        <item name="android:background">@drawable/btn_rounded_purple_background</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:paddingStart">@dimen/spacing_m</item>
        <item name="android:paddingEnd">@dimen/spacing_l</item>
        <item name="android:drawableTint">@color/white_invertible</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:drawablePadding">@dimen/spacing_xs</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">2</item>
        <item name="android:imeOptions">actionSearch</item>
    </style>

    <style name="TabButton">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:minHeight">@dimen/btn_height</item>
        <item name="android:textColor">@color/btn_tab_text</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:layout_height">@dimen/btn_height</item>
        <item name="android:lines">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ImageButton" parent="TextAppearance.AppCompat.Widget.Button">
        <item name="android:textAllCaps">false</item>
        <item name="android:padding">@dimen/spacing_m</item>
        <item name="android:drawablePadding">@dimen/spacing_xs</item>
        <item name="android:background">?selectableItemBackground</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ListingDisplayModeToggle" parent="ExtraSmallBody">
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/listing_display_mode_toggle_text</item>
        <item name="android:drawableTint">@color/listing_display_mode_toggle_text</item>
        <item name="android:paddingTop">@dimen/spacing_xs</item>
        <item name="android:paddingBottom">@dimen/spacing_xs</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="RadioButton" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:textColor">@color/black_invertible</item>
        <item name="buttonTint">@color/radio_button</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:paddingTop">@dimen/spacing_s</item>
        <item name="android:paddingBottom">@dimen/spacing_s</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="SubFloatingActionButton" parent="Widget.Design.FloatingActionButton">
        <item name="borderWidth">@dimen/button_border</item>
        <item name="android:backgroundTint">@color/white_invertible</item>
        <item name="android:tint">@color/cyan</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:scaleType">centerInside</item>
        <item name="rippleColor">@color/transparent</item>
        <item name="android:iconTint" tools:targetApi="o">@color/cyan</item>
        <item name="maxImageSize">@dimen/ic_bottom_main_action_radius</item>
        <item name="fabCustomSize">@dimen/ic_bottom_main_action_size</item>
    </style>

    <style name="Pill" parent="Body">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">@drawable/pill</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingStart">@dimen/spacing_m</item>
        <item name="android:paddingEnd">@dimen/spacing_m</item>
    </style>

    <style name="SmallPill" parent="SmallBody">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">@drawable/small_pill</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingStart">@dimen/spacing_m</item>
        <item name="android:paddingEnd">@dimen/spacing_m</item>
    </style>

    <style name="SmallPill.Accent" parent="SmallPill">
        <item name="android:background">@drawable/small_pill_accent</item>
        <item name="android:textColor">@color/text_pill_accent</item>
    </style>

    <style name="SpinnerRectangle" parent="Widget.AppCompat.Spinner">
        <item name="android:background">@drawable/view_gray_border_white_background</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textColor">@color/black_invertible</item>
        <item name="android:textStyle">normal</item>
        <item name="android:drawableEnd">@drawable/ic_arrow_down</item>
    </style>

    <style name="CheckBox" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="colorAccent">@color/cyan</item>
        <item name="android:buttonTint">@color/checkbox_tint</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="ProgressBar" parent="Widget.AppCompat.ProgressBar">
        <item name="android:indeterminateTint">@color/cyan</item>
    </style>

    <style name="TabButtonLeft" parent="TabButton">
        <item name="android:background">@drawable/btn_tab_left</item>
    </style>

    <style name="TabButtonRight" parent="TabButton">
        <item name="android:background">@drawable/btn_tab_right</item>
    </style>

    <style name="TabButtonCenter" parent="TabButton">
        <item name="android:background">@drawable/btn_tab_center</item>
    </style>

    <style name="Switch">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:thumbTint">@color/checkbox_tint</item>
        <item name="thumbTint">@color/checkbox_tint</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:switchPadding">@dimen/spacing_xs</item>
        <item name="switchPadding">@dimen/spacing_xs</item>
        <item name="android:textColor">@color/black_invertible</item>
    </style>

    <!-- Custom Views -->
    <style name="AppCardView" parent="CardView">
        <item name="cardBackgroundColor">@color/white_invertible</item>
        <item name="strokeColor">@color/card_border</item>
        <item name="strokeWidth">@dimen/list_view_divider</item>
        <item name="cardCornerRadius">@dimen/card_border_radius</item>
        <item name="contentPaddingTop">@dimen/spacing_m</item>
        <item name="contentPaddingBottom">@dimen/spacing_l</item>
        <item name="contentPaddingLeft">@dimen/spacing_m</item>
        <item name="contentPaddingRight">@dimen/spacing_m</item>
    </style>

    <style name="Badge" parent="ExtraSmallBody">
